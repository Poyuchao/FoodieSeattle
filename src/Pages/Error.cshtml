@page
@model ErrorModel
<!-- Error page to show, contains error messages to render if request cannot be completed-->
@{
    ViewData["Title"] = "Error";
}
<div class="container">
    <div id="not-found">
        <div class="not-found">
            <div class="not-found-404">
                <h1>Oops! Not a valid Restaurant ID</h1>
                <h2>404 - The page cannot be found</h2>
            </div>
            <a href="/">Go To Homepage</a>
        </div>
    </div>
</div>